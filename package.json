{
  "title": "KSMCounseling",
  "name": "ksmcounseling",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:sass && eleventy",
    "clean": "rm -rf dist/**",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build:sass": "sass --style=compressed src/scss/styles.scss:src/css/styles.css",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "npm run build:sass -- --watch"
  },
  "description": "KSMCounseling Play Therapy",
  "homepage": "https://www.letsplaycounseling.com",
  "license": "MIT",
  "author": "Kyle McNally <kyle@kmcnally.net>",
  "contributors": [
    "David Miller (https://davidmiller.io/)",
    "Start Bootstrap (https://github.com/StartBootstrap/startbootstrap-agency.git)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sparticuz/ksmcounseling.git"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "bootstrap": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.1",
    "sass": "^1.43.4"
  }
}
